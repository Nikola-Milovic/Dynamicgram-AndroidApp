<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/app_navigation_graph"
    app:startDestination="@id/homefeed_nav_graph">

    <include app:graph = "@navigation/homefeed_nav_graph"/>

    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.nikolam.feature_explore.ExploreFragment"
        android:label="fragment_explore"
        app:moduleName="feature_explore" />

    <include-dynamic
        android:id="@+id/includedGraph"
        app:graphPackage="com.nikolam.feature_explore"
        app:graphResName="feature_explore_nav_graph"
        app:moduleName="feature_explore" />

</navigation>
